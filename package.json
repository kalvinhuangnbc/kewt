    "codecov": "^1.0.1",
    "emdaer": "^0.3.3",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-ava": "^4.0.0",
    "eslint-plugin-import": "^1.14.0",
    "jsdom": "^9.5.0",
    "lodash.defaults": "^4.2.0",
    "nyc": "^10.0.0"
  }
